ur temperature circles */\n\n  // .append('path') because we only want ONE path\n  // .datum because we only have ONE path\n  svg\n    .append('path')\n    .datum(datapoints)\n    .attr('stroke', 'salmon')\n    .attr('stroke-width', 2)\n    .attr('d', line)\n    .attr('fill', 'none')\n\n  svg\n    .selectAll('circle')\n    .data(datapoints)\n    .enter()\n    .append('circle')\n    .attr('fill', 'salmon')\n    .attr('r', 3)\n    .attr('cx', d => {\n      return xPositionScale(d.day)\n    })\n    .attr('cy', d => {\n      return yPositionScale(d.temperature)\n    })\n\n  /* Add in your axes */\n\n  const xAxis = d3.axisBottom(xPositionScale)\n  svg\n    .append('g')\n    .attr('class', 'axis x-axis')\n    .attr('transform', 'translate(0,' + height + ')')\n    .call(xAxis)\n\n  const yAxis = d3.axisLeft(yPositionScale)\n  svg\n    .append('g')\n    .attr('class', 'axis y-axis')\n    .call(yAxis)\n}"},"lineCount":null}},"hash":"778939b069de828a120fc8c1ea3404d3","cacheData":{"env":{}}}